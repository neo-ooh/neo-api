{
  "name": "neo/api",
  "type": "project",
  "description": "Neo-OOH web services API.",
  "keywords": [],
  "license": "",
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/fedeisas/laradoo.git"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "arkaitzgarro/elastic-apm-laravel": "^3.0",
    "bensampo/laravel-enum": "^3.1",
    "doctrine/dbal": "3.1.1",
    "edujugon/laradoo": "dev-php_8",
    "fideloper/proxy": "^4.4",
    "firebase/php-jwt": "^5.2",
    "fruitcake/laravel-cors": "^2.0",
    "grimzy/laravel-mysql-spatial": "^5.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "intervention/image": "^2.5",
    "itsgoingd/clockwork": "^5.0",
    "laravel/framework": "^8.12",
    "laravel/telescope": "^4.6",
    "laravel/tinker": "^2.5",
    "league/csv": "^9.6",
    "league/flysystem-aws-s3-v3": "^1.0",
    "loilo/fuse": "^4.0",
    "mpdf/mpdf": "^8.0",
    "php-ffmpeg/php-ffmpeg": "^0.18.0",
    "php-http/guzzle7-adapter": "^1.0",
    "phpoffice/phpspreadsheet": "^1.18",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.1",
    "spatie/image": "^1.10",
    "toin0u/geocoder-laravel": "^4.4"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "nunomaduro/larastan": "^0.7.12",
    "phpstan/phpstan": "^0.12.96",
    "phpunit/phpunit": "^9.3.3",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "Neo\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "migrate": [
      "php artisan migrate",
      "php artisan db:seed --force"
    ]
  }
}
