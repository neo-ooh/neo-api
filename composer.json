{
  "name": "neo/api",
  "type": "project",
  "description": "Neo-OOH web services API.",
  "keywords": [],
  "license": "",
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/neo-ooh/laradoo.git"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-zlib": "*",
    "ajcastro/eager-load-pivot-relations": "^0.3.0",
    "aloha/twilio": "^5.0",
    "doctrine/dbal": "^3.1.4",
    "edujugon/laradoo": "dev-laravel10",
    "firebase/php-jwt": "^6",
    "geocoder-php/geonames-provider": "^4.4",
    "gotenberg/gotenberg-php": "^1.1",
    "guzzlehttp/guzzle": "^7.0.1",
    "halaxa/json-machine": "^1.1",
    "hannesvdvreken/guzzle-clockwork": "4.0.0-RC1",
    "intervention/image": "^2.5",
    "itsgoingd/clockwork": "^5.1",
    "jmikola/geojson": "^1.0",
    "laravel/framework": "^10.0",
    "laravel/tinker": "^2.5",
    "league/csv": "^9.6",
    "league/flysystem-async-aws-s3": "^3.10",
    "league/flysystem-aws-s3-v3": "^3.0",
    "league/flysystem-ftp": "^3.0",
    "league/uri": "^6.7",
    "league/uri-components": "^2.4",
    "loilo/fuse": "^6.0",
    "matanyadaev/laravel-eloquent-spatial": "^3.1",
    "mpdf/mpdf": "^8.1.1",
    "nwidart/laravel-modules": "^10.0",
    "php-ffmpeg/php-ffmpeg": "^1.0",
    "php-http/guzzle7-adapter": "^1.0",
    "phpoffice/phpspreadsheet": "^1.18",
    "phpxmlrpc/polyfill-xmlrpc": "^1.0@RC",
    "predis/predis": "^2.0.0",
    "propaganistas/laravel-phone": "^5.0.3",
    "ramsey/uuid": "^4.1",
    "skagarwal/google-places-api": "^2.0",
    "spatie/image": "^2.2",
    "spatie/laravel-data": "^3.0",
    "staudenmeir/eloquent-has-many-deep": "^1.15",
    "symfony/expression-language": "^6.0.1",
    "toin0u/geocoder-laravel": "^4.4",
    "vinkla/hashids": "^11.0.0"
  },
  "require-dev": {
    "enlightn/enlightn": "^2.3",
    "fakerphp/faker": "^1.9.1",
    "jetbrains/phpstorm-attributes": "^1.0",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^7.0",
    "nunomaduro/larastan": "^2.0",
    "phpstan/phpstan": "^1",
    "phpunit/phpunit": "^10.0",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "Neo\\": "app/",
      "Neo\\Modules\\": "Modules/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "migrate": [
      "php artisan migrate",
      "php artisan db:seed --force"
    ]
  }
}
