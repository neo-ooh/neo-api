{
  "name": "neo/api",
  "type": "project",
  "description": "Neo-OOH web services API.",
  "keywords": [],
  "license": "",
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/neo-ooh/laradoo.git"
    },
    {
      "type": "github",
      "url": "https://github.com/neo-ooh/laravel-mysql-spatial.git"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "aloha/twilio": "^5.0",
    "bensampo/laravel-enum": "^5",
    "doctrine/dbal": "^3.1.4",
    "edujugon/laradoo": "dev-laravel9",
    "firebase/php-jwt": "^6",
    "fruitcake/laravel-cors": "^3.0",
    "grimzy/laravel-mysql-spatial": "dev-laravel9",
    "guzzlehttp/guzzle": "^7.0.1",
    "hannesvdvreken/guzzle-clockwork": "4.0.0-RC1",
    "intervention/image": "^2.5",
    "itsgoingd/clockwork": "^5.1",
    "laravel/framework": "^9.9",
    "laravel/tinker": "^2.5",
    "league/csv": "^9.6",
    "league/flysystem-aws-s3-v3": "^3.0",
    "league/flysystem-ftp": "^3.0",
    "loilo/fuse": "^6.0",
    "mpdf/mpdf": "^8.1.1",
    "nwidart/laravel-modules": "^9.0",
    "php-ffmpeg/php-ffmpeg": "^1.0",
    "php-http/guzzle7-adapter": "^1.0",
    "phpoffice/phpspreadsheet": "^1.18",
    "predis/predis": "^2.0.0",
    "propaganistas/laravel-phone": "^4.3",
    "ramsey/uuid": "^4.1",
    "skagarwal/google-places-api": "^2.0",
    "spatie/data-transfer-object": "^3.8",
    "spatie/image": "^2.2",
    "symfony/expression-language": "^6.0.1",
    "toin0u/geocoder-laravel": "^4.4",
    "vinkla/hashids": "^10.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "jetbrains/phpstorm-attributes": "^1.0",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^6.1",
    "nunomaduro/larastan": "^2.0",
    "phpstan/phpstan": "^1",
    "phpunit/phpunit": "^9.3.3",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "Neo\\": "app/",
      "Neo\\Modules\\": "Modules/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "migrate": [
      "php artisan migrate",
      "php artisan db:seed --force"
    ]
  }
}
